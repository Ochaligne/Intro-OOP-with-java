            <Button fx:id="PutItem" layoutX="466.0" layoutY="182.0" mnemonicParsing="false" onAction="#putItem" prefHeight="59.0" prefWidth="82.0" text="Put the Item" />
      </children>
    </AnchorPane>
  </children>
</VBox>
