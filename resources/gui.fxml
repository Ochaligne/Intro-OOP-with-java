    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Button fx:id="ButtonLeft" layoutX="30.0" layoutY="333.0" mnemonicParsing="false" onAction="#leftClicked" prefHeight="25.0" prefWidth="60.0" text="Left" AnchorPane.topAnchor="333.0" />
            <Button fx:id="ButtonDoor" layoutX="98.0" layoutY="333.0" mnemonicParsing="false" onAction="#doorClicked" prefHeight="25.0" prefWidth="101.0" text="Through Door" AnchorPane.topAnchor="333.0" />
            <Button fx:id="ButtonRight" layoutX="205.0" layoutY="333.0" mnemonicParsing="false" onAction="#rightClicked" prefHeight="25.0" prefWidth="69.0" text="Right" AnchorPane.topAnchor="333.0" />
            <ImageView fx:id="RoomView" fitHeight="275.0" fitWidth="512.0" layoutX="13.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" AnchorPane.bottomAnchor="111.0" AnchorPane.leftAnchor="13.0" AnchorPane.rightAnchor="12.0" AnchorPane.topAnchor="14.0" />
            <ImageView fx:id="ItemView" fitHeight="213.0" fitWidth="118.0" layoutX="215.0" layoutY="28.0" pickOnBounds="true" preserveRatio="true" AnchorPane.bottomAnchor="159.0" AnchorPane.leftAnchor="215.0" AnchorPane.rightAnchor="307.0" AnchorPane.topAnchor="28.0" />
